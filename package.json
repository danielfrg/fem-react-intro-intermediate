{
  "name": "fem",
  "version": "1.0.0",
  "description": "Frontend Masters class",
  "repository": {
    "type": "git",
    "url": "https://github.com/danielfrg/react-class-v5.git"
  },
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf .cache dist coverage",
    "cleanall": "npm run clean && rm -rf node_modules",
    "dev": "parcel src/index.html",
    "build": "parcel build --public-url ./dist/ src/index.html",
    "fmt": "prettier --write \"src/**/*.{js,jsx,css,json}\"",
    "fmt:check": "prettier --list-different \"src/**/*.{js,jsx,css,json}\"",
    "lint": "eslint \"src/**/*.{js,jsx }\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:update": "jest -u"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.0.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "jest": "^26.0.1",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.0.5",
    "react-test-renderer": "^16.13.1"
  },
  "dependencies": {
    "@emotion/babel-preset-css-prop": "^10.0.27",
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "@reach/router": "^1.3.3",
    "cross-env": "^7.0.2",
    "petfinder-client": "^1.0.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-emotion": "^10.0.0",
    "react-loadable": "^5.5.0"
  },
  "browserslist": [
    "last 2 Chrome versions"
  ]
}
